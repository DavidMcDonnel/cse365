/**
 * Should print the number 7 four times.
 * Requires translation of 
 *     - expression with multiple terms/ops
 *     - term / ( expression )
 *     - term / - term
 *     - term / ~ term
 *     - op
 */
class Main {

   function void main() {
       do Output.printInt(1 + (2 * 3));
       do Output.println();
       do Output.printInt(50 - 1 / 7);
       do Output.println();
       do Output.printInt(27 & 67 | 4);
       do Output.println();
       do Output.printInt(~-8);

       return;
       
   }

}
